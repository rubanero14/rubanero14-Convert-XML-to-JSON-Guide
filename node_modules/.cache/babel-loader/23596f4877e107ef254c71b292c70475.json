{"ast":null,"code":"import axios from 'axios';\nimport sources from '@/assets/sources.js';\n\nconst xml2js = require('xml2js');\n\nexport default {\n  data() {\n    return {\n      data: '',\n      isloading: false,\n      isError: false,\n      rssSource: '',\n      screenWidth: '',\n      tabNav: 0,\n      topicData: '',\n      topicNavUrl: '',\n      topicTitle: '',\n      topicTitle2: '',\n      sources: sources\n    };\n  },\n\n  watch: {\n    url() {\n      this.getRssFeeds();\n      this.setScreenWidth();\n    }\n\n  },\n\n  mounted() {\n    window.addEventListener('resize', this.setScreenWidth);\n    this.setScreenWidth();\n  },\n\n  unmounted() {\n    window.removeEventListener('resize', this.setScreenWidth);\n  },\n\n  methods: {\n    forwardNav(data) {\n      if (this.tabNav === 0 && data.topics.length > 1) {\n        this.topicData = data.topics;\n        this.topicNavUrl = data.url;\n        this.topicTitle = data.name;\n        return this.tabNav < 3 ? this.tabNav++ : this.tabNav;\n      }\n\n      if (this.tabNav === 0 && data.topics.length === 1) {\n        this.pic = data.url;\n        this.topicData = data.topics;\n        this.topicNavUrl = data.topics[0].url;\n        this.topicTitle2 = data.topics[0].title;\n        this.getRssFeeds(this.pic, this.topicNavUrl, this.topicTitle2);\n        return this.tabNav = 2;\n      }\n    },\n\n    backwardNav(err) {\n      if (this.tabNav === 2 && this.topicData.length === 1) {\n        return this.tabNav = 0;\n      }\n\n      if (err) {\n        this.isError = false;\n        console.log('Iserror ', this.isError);\n        return this.tabNav = 0;\n      }\n\n      return this.tabNav > -1 ? this.tabNav-- : this.tabNav;\n    },\n\n    setScreenWidth() {\n      console.log(window.innerWidth);\n      return this.screenWidth = window.innerWidth;\n    },\n\n    async getRssFeeds(picUrl, payloadUrl, title) {\n      this.tabNav = 2;\n      this.isError = false;\n      this.isloading = true;\n      this.pic = picUrl;\n      this.topicTitle2 = title; // Payload for Fetch API setting\n\n      let payload = `https://cors-anywhere.herokuapp.com/${payloadUrl}`; // Fetch API as XML and convert into JSON format\n\n      this.data = await axios.get(payload).then(response => {\n        return xml2js.parseStringPromise(response.data);\n      }).catch(err => {\n        this.isloading = false;\n        this.isError = true;\n        console.log(err);\n        return err.message + ',';\n      });\n      this.feeds = this.data.rss.channel[0].item; // Custom show elapsed time algorithm\n\n      this.date = () => {\n        // get UNIX timestamp of pubDate value\n        let Elapsed = new Date(this.feeds[0].pubDate).getTime(); // get UNIX timestamp of Present value\n\n        let Now = Date.now(); // Convert UNIX to seconds\n\n        let seconds = (Now - Elapsed) / 1000; // Convert UNIX to minutes\n\n        let minutes = seconds / 60; // Convert UNIX to hours\n\n        let hours = minutes / 60; // Convert UNIX to days\n\n        let days = hours / 24; // Convert UNIX to weeks\n\n        let weeks = days / 7; // Convert UNIX to weeks\n\n        let months = weeks / 4; // Convert UNIX to weeks\n\n        let years = months / 12; // Elapsed Time output logic\n\n        if (seconds >= 1 && seconds < 60) {\n          return seconds.toFixed(0) + (seconds.toFixed(0) < 2 ? ' second' : ' seconds');\n        }\n\n        if (minutes >= 1 && minutes < 60) {\n          return minutes.toFixed(0) + (minutes.toFixed(0) < 2 ? ' minute' : ' minutes');\n        }\n\n        if (hours >= 1 && hours < 24) {\n          return hours.toFixed(0) + (hours.toFixed(0) < 2 ? ' hour' : ' hours');\n        }\n\n        if (days >= 1 && days < 7) {\n          return days.toFixed(0) + (days.toFixed(0) < 2 ? ' day' : ' days');\n        }\n\n        if (weeks >= 1 && weeks < 4) {\n          return weeks.toFixed(0) + (weeks.toFixed(0) < 2 ? ' week' : ' weeks');\n        }\n\n        if (months >= 1 && months < 12) {\n          return months.toFixed(0) + (months.toFixed(0) < 2 ? ' month' : ' months');\n        }\n\n        if (years >= 1) {\n          return years.toFixed(0) + (years.toFixed(0) < 2 ? ' year' : ' years');\n        }\n      };\n\n      this.isloading = false;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA2JA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,OAAP,MAAoB,qBAApB;;AAEA,MAAMC,MAAK,GAAIC,OAAO,CAAC,QAAD,CAAtB;;AAEA,eAAe;EACbC,IAAI,GAAE;IACJ,OAAO;MACLA,IAAI,EAAE,EADD;MAELC,SAAS,EAAE,KAFN;MAGLC,OAAO,EAAE,KAHJ;MAILC,SAAS,EAAE,EAJN;MAKLC,WAAW,EAAE,EALR;MAMLC,MAAM,EAAE,CANH;MAOLC,SAAS,EAAC,EAPL;MAQLC,WAAW,EAAE,EARR;MASLC,UAAU,EAAE,EATP;MAULC,WAAW,EAAC,EAVP;MAWLZ,OAAO,EAAEA;IAXJ,CAAP;EAaD,CAfY;;EAgBba,KAAK,EAAE;IACLC,GAAG,GAAE;MACH,KAAKC,WAAL;MACA,KAAKC,cAAL;IACD;;EAJI,CAhBM;;EAsBbC,OAAO,GAAG;IACRC,MAAM,CAACC,gBAAP,CAAwB,QAAxB,EAAkC,KAAKH,cAAvC;IACA,KAAKA,cAAL;EACD,CAzBY;;EA0BbI,SAAS,GAAG;IACVF,MAAM,CAACG,mBAAP,CAA2B,QAA3B,EAAqC,KAAKL,cAA1C;EACD,CA5BY;;EA6BbM,OAAO,EAAE;IACPC,UAAU,CAACpB,IAAD,EAAM;MACd,IAAG,KAAKK,MAAL,KAAgB,CAAhB,IAAqBL,IAAI,CAACqB,MAAL,CAAYC,MAAZ,GAAqB,CAA7C,EAA+C;QAC7C,KAAKhB,SAAL,GAAiBN,IAAI,CAACqB,MAAtB;QACA,KAAKd,WAAL,GAAmBP,IAAI,CAACW,GAAxB;QACA,KAAKH,UAAL,GAAkBR,IAAI,CAACuB,IAAvB;QACA,OAAO,KAAKlB,MAAL,GAAc,CAAd,GAAkB,KAAKA,MAAL,EAAlB,GAAkC,KAAKA,MAA9C;MACF;;MAEA,IAAG,KAAKA,MAAL,KAAgB,CAAhB,IAAqBL,IAAI,CAACqB,MAAL,CAAYC,MAAZ,KAAuB,CAA/C,EAAiD;QAC/C,KAAKE,GAAL,GAAWxB,IAAI,CAACW,GAAhB;QACA,KAAKL,SAAL,GAAiBN,IAAI,CAACqB,MAAtB;QACA,KAAKd,WAAL,GAAmBP,IAAI,CAACqB,MAAL,CAAY,CAAZ,EAAeV,GAAlC;QACA,KAAKF,WAAL,GAAmBT,IAAI,CAACqB,MAAL,CAAY,CAAZ,EAAeI,KAAlC;QACA,KAAKb,WAAL,CAAiB,KAAKY,GAAtB,EAA0B,KAAKjB,WAA/B,EAA2C,KAAKE,WAAhD;QACA,OAAO,KAAKJ,MAAL,GAAc,CAArB;MACF;IACD,CAjBM;;IAkBPqB,WAAW,CAACC,GAAD,EAAK;MACd,IAAG,KAAKtB,MAAL,KAAgB,CAAhB,IAAqB,KAAKC,SAAL,CAAegB,MAAf,KAA0B,CAAlD,EAAoD;QAClD,OAAO,KAAKjB,MAAL,GAAc,CAArB;MACF;;MAEA,IAAGsB,GAAH,EAAO;QACL,KAAKzB,OAAL,GAAe,KAAf;QACA0B,OAAO,CAACC,GAAR,CAAY,UAAZ,EAAuB,KAAK3B,OAA5B;QACA,OAAO,KAAKG,MAAL,GAAc,CAArB;MACF;;MAEA,OAAO,KAAKA,MAAL,GAAc,CAAC,CAAf,GAAmB,KAAKA,MAAL,EAAnB,GAAmC,KAAKA,MAA/C;IACD,CA9BM;;IA+BPQ,cAAc,GAAE;MACde,OAAO,CAACC,GAAR,CAAYd,MAAM,CAACe,UAAnB;MACA,OAAO,KAAK1B,WAAL,GAAmBW,MAAM,CAACe,UAAjC;IACD,CAlCM;;IAmCP,MAAMlB,WAAN,CAAkBmB,MAAlB,EAA0BC,UAA1B,EAAsCP,KAAtC,EAA4C;MAC1C,KAAKpB,MAAL,GAAc,CAAd;MACA,KAAKH,OAAL,GAAe,KAAf;MACA,KAAKD,SAAL,GAAiB,IAAjB;MACA,KAAKuB,GAAL,GAAWO,MAAX;MACA,KAAKtB,WAAL,GAAmBgB,KAAnB,CAL0C,CAO1C;;MACA,IAAIQ,OAAM,GAAK,uCAAsCD,UAAW,EAAhE,CAR0C,CAU1C;;MACA,KAAKhC,IAAL,GAAY,MAAMJ,KAAI,CACrBsC,GADiB,CACbD,OADa,EAEjBE,IAFiB,CAEXC,QAAD,IAAc;QAClB,OAAOtC,MAAM,CAACuC,kBAAP,CAA0BD,QAAQ,CAACpC,IAAnC,CAAP;MACD,CAJiB,EAKjBsC,KALiB,CAKXX,GAAE,IAAK;QACZ,KAAK1B,SAAL,GAAiB,KAAjB;QACA,KAAKC,OAAL,GAAe,IAAf;QACA0B,OAAO,CAACC,GAAR,CAAYF,GAAZ;QACA,OAAOA,GAAG,CAACY,OAAJ,GAAc,GAArB;MACD,CAViB,CAAlB;MAYA,KAAKC,KAAL,GAAa,KAAKxC,IAAL,CAAUyC,GAAV,CAAcC,OAAd,CAAsB,CAAtB,EAAyBC,IAAtC,CAvB0C,CAyB1C;;MACA,KAAKC,IAAL,GAAY,MAAM;QAChB;QACA,IAAIC,OAAM,GAAI,IAAIC,IAAJ,CAAS,KAAKN,KAAL,CAAW,CAAX,EAAcO,OAAvB,EAAgCC,OAAhC,EAAd,CAFgB,CAIhB;;QACA,IAAIC,GAAE,GAAIH,IAAI,CAACI,GAAL,EAAV,CALgB,CAOhB;;QACA,IAAIC,OAAM,GAAK,CAACF,GAAE,GAAIJ,OAAP,IAAkB,IAAjC,CARgB,CAUhB;;QACA,IAAIO,OAAM,GAAKD,OAAM,GAAI,EAAzB,CAXgB,CAahB;;QACA,IAAIE,KAAI,GAAKD,OAAM,GAAI,EAAvB,CAdgB,CAgBhB;;QACA,IAAIE,IAAG,GAAKD,KAAI,GAAI,EAApB,CAjBgB,CAmBhB;;QACA,IAAIE,KAAI,GAAKD,IAAG,GAAI,CAApB,CApBgB,CAsBhB;;QACA,IAAIE,MAAK,GAAKD,KAAI,GAAI,CAAtB,CAvBgB,CAyBhB;;QACA,IAAIE,KAAI,GAAKD,MAAK,GAAI,EAAtB,CA1BgB,CA4BhB;;QACA,IAAGL,OAAM,IAAK,CAAX,IAAgBA,OAAM,GAAI,EAA7B,EAAgC;UAC9B,OAAOA,OAAO,CAACO,OAAR,CAAgB,CAAhB,KAAsBP,OAAO,CAACO,OAAR,CAAgB,CAAhB,IAAqB,CAArB,GAAyB,SAAzB,GAAmC,UAAzD,CAAP;QACF;;QAEA,IAAGN,OAAM,IAAK,CAAX,IAAgBA,OAAM,GAAI,EAA7B,EAAgC;UAC9B,OAAOA,OAAO,CAACM,OAAR,CAAgB,CAAhB,KAAsBN,OAAO,CAACM,OAAR,CAAgB,CAAhB,IAAqB,CAArB,GAAyB,SAAzB,GAAmC,UAAzD,CAAP;QACF;;QAEA,IAAGL,KAAI,IAAK,CAAT,IAAcA,KAAI,GAAI,EAAzB,EAA4B;UAC1B,OAAOA,KAAK,CAACK,OAAN,CAAc,CAAd,KAAoBL,KAAK,CAACK,OAAN,CAAc,CAAd,IAAmB,CAAnB,GAAuB,OAAvB,GAA+B,QAAnD,CAAP;QACF;;QAEA,IAAGJ,IAAG,IAAK,CAAR,IAAaA,IAAG,GAAI,CAAvB,EAAyB;UACvB,OAAOA,IAAI,CAACI,OAAL,CAAa,CAAb,KAAmBJ,IAAI,CAACI,OAAL,CAAa,CAAb,IAAkB,CAAlB,GAAsB,MAAtB,GAA6B,OAAhD,CAAP;QACF;;QAEA,IAAGH,KAAI,IAAK,CAAT,IAAcA,KAAI,GAAI,CAAzB,EAA2B;UACzB,OAAOA,KAAK,CAACG,OAAN,CAAc,CAAd,KAAoBH,KAAK,CAACG,OAAN,CAAc,CAAd,IAAmB,CAAnB,GAAuB,OAAvB,GAA+B,QAAnD,CAAP;QACF;;QAEA,IAAGF,MAAK,IAAK,CAAV,IAAeA,MAAK,GAAI,EAA3B,EAA8B;UAC5B,OAAOA,MAAM,CAACE,OAAP,CAAe,CAAf,KAAqBF,MAAM,CAACE,OAAP,CAAe,CAAf,IAAoB,CAApB,GAAwB,QAAxB,GAAiC,SAAtD,CAAP;QACF;;QAEA,IAAGD,KAAI,IAAK,CAAZ,EAAc;UACZ,OAAOA,KAAK,CAACC,OAAN,CAAc,CAAd,KAAoBD,KAAK,CAACC,OAAN,CAAc,CAAd,IAAmB,CAAnB,GAAuB,OAAvB,GAA+B,QAAnD,CAAP;QACF;MACD,CAxDD;;MAyDA,KAAKzD,SAAL,GAAiB,KAAjB;IACD;;EAvHM;AA7BI,CAAf","names":["axios","sources","xml2js","require","data","isloading","isError","rssSource","screenWidth","tabNav","topicData","topicNavUrl","topicTitle","topicTitle2","watch","url","getRssFeeds","setScreenWidth","mounted","window","addEventListener","unmounted","removeEventListener","methods","forwardNav","topics","length","name","pic","title","backwardNav","err","console","log","innerWidth","picUrl","payloadUrl","payload","get","then","response","parseStringPromise","catch","message","feeds","rss","channel","item","date","Elapsed","Date","pubDate","getTime","Now","now","seconds","minutes","hours","days","weeks","months","years","toFixed"],"sourceRoot":"","sources":["C:\\Users\\Raj\\Desktop\\RSS Feed\\rss-feed\\src\\components\\RSSFeed.vue"],"sourcesContent":["<template>\r\n  <div class=\"container\">\r\n    <div class=\"row\">\r\n      <div :class=\"{'col-3':screenWidth >= 1200, '':screenWidth < 1200}\"></div>\r\n      <div :class=\"{'':screenWidth < 1200, 'col-6':screenWidth >= 1200}\">\r\n        <!-- Header Section -->\r\n        <h1 class=\"text-secondary mb-3\"><strong>RSS Feed - Vue 3</strong></h1>\r\n\r\n        <a v-if=\"tabNav === 0\" href=\"https://github.com/rubanero14/rubanero14-Convert-XML-to-JSON-Guide/blob/master/src/components/RSSFeed.vue\" class=\"btn btn-outline-secondary mb-lg-3 w-100\" target=\"_blank\"><i class=\"bi bi-code-slash\"></i> Source Code</a>\r\n        \r\n        <button v-if=\"tabNav > 0\" @click=\"backwardNav(isError)\" class=\"btn btn-secondary w-100\" :disabled=\"isloading\"><i class=\"bi bi-arrow-left\"></i> Back</button>\r\n        \r\n        <hr class=\"my-3\" size=\"5\" noshade/>\r\n        \r\n        <!-- Loading Spinner Section -->\r\n        <div v-if=\"isloading\" class=\"text-center spinner-border text-secondary\" role=\"status\">\r\n        </div>\r\n        \r\n        <!-- Sources Tiles Section -->\r\n        <Transition name=\"card-fade\" appear mode=\"out-in\">\r\n          <div>\r\n            <Transition name=\"fade\" appear mode=\"out-in\">\r\n              <h2  v-if=\"!isloading && tabNav === 0\" class=\"text-secondary mb-3\">Sources</h2>\r\n            </Transition>\r\n            <div>\r\n              <!-- Declaring and assigning index using v-for and use it to assign as key -->\r\n              <div v-for=\"(source, index) in sources\" :key=\"index\" class=\"d-inline-block\">\r\n                <Transition \r\n                  appear\r\n                  :key=\"index\"\r\n                  name=\"fade\" \r\n                  mode=\"out-in\"\r\n                >\r\n                  <!-- Using the declared index and assign it to dynamic variable for CSS transition use -->\r\n                  <center  v-if=\"!isloading && tabNav === 0\" :style=\"{ '--i':index }\">\r\n                    <a class=\"title\" @click=\"forwardNav(source)\">\r\n                      <div class=\"card logo p-0 mb-3 mx-2\">\r\n                        <div class=\"d-inline-block justify-content-center align-items-center\">\r\n                          <img class=\"logo\" :src=\"source.logo\" onerror=\"this.src='https://rss.com/favicon.ico'\"/>\r\n                        </div>\r\n                      </div>\r\n                    </a>\r\n                  </center>\r\n                </Transition>\r\n              </div> \r\n            </div>\r\n          </div>\r\n        </Transition>\r\n        \r\n        \r\n        <!-- Articles Section -->\r\n        <div style=\"margin-top: -20px;\">\r\n          <Transition name=\"fade\" appear mode=\"out-in\">\r\n            <h2 v-if=\"!isError && !isloading && tabNav === 2\" class=\"text-secondary mb-3\">{{ topicTitle2 }}</h2>\r\n          </Transition>\r\n          <!-- Declaring and assigning index using v-for and use it to assign as key -->\r\n          <div class=\"mb-2\" v-for=\"(feed, index) in feeds\" :key=\"index\">\r\n            <Transition\r\n              :key=\"index\"\r\n              appear\r\n              name=\"fade-articles\"\r\n              mode=\"out-in\"\r\n            >\r\n              <!-- Using the declared index and assign it to dynamic variable for CSS transition use -->\r\n              <center v-if=\"!isError && !isloading && tabNav === 2\" :style=\"{ '--j':index }\">\r\n                <a class=\"title\" :href=\"feed.link.toString()\">\r\n                  <div class=\"card\">\r\n                    <div class=\"p-3\">\r\n                      <div class=\"row\">\r\n                        <div class=\"col-9 col-md-11 text-start\">\r\n                          <h3 class=\"title text-secondary\">\r\n                            <span v-if=\"screenWidth >= 1200\">{{ feed.title.toString().substr(0, 250).replace(': ','') + '...' }}</span>\r\n                            <span v-else-if=\"screenWidth >= 600 && screenWidth < 1200\">{{ feed.title.toString().substr(0, 150).replace(': ','') + '...' }}</span>\r\n                            <span v-else>{{ feed.title.toString().substr(0, 50).replace(': ','') + '...' }}</span>\r\n                          </h3>\r\n                          <p v-if=\"date()\" class=\"time d-block text-secondary mb-0\"><em>Updated: {{ date() }} ago</em></p>\r\n                        </div>\r\n                        <div class=\"wrapper col-3 col-md-1 d-flex align-items-center justify-content-center\">\r\n                          <img class=\"m-auto\" v-if=\"pic\" :src=\"pic\" onerror=\"this.src='https://rss.com/favicon.ico'\"/>\r\n                        </div>\r\n                      </div>\r\n                    </div>\r\n                  </div>\r\n                </a>\r\n              </center>\r\n            </Transition>\r\n          </div>\r\n        </div>\r\n        \r\n        <!-- Topic Tiles Section -->\r\n        <Transition name=\"card-fade\" appear mode=\"out-in\">\r\n            <div style=\"margin-top: -20px;\">\r\n              <Transition name=\"fade\" appear mode=\"out-in\">\r\n                <h2 v-if=\"!isloading && tabNav === 1\" class=\"text-secondary mb-3\">{{ topicTitle }}</h2>\r\n              </Transition>\r\n              <div>\r\n                  <!-- Declaring and assigning index using v-for and use it to assign as key -->\r\n                  <div v-for=\"(topic,index) in topicData\" :key=\"index\" class=\"d-inline-block\">\r\n                    <Transition\r\n                      :key=\"index\"\r\n                      appear\r\n                      name=\"fade\"\r\n                      mode=\"out-in\"\r\n                    >\r\n                      <!-- Using the declared index and assign it to dynamic variable for CSS transition use -->\r\n                      <center v-if=\"!isloading && tabNav === 1\" :style=\"{ '--i':index }\">\r\n                        <a class=\"title\" @click=\"getRssFeeds(topicNavUrl,topic.url,topic.title) && forwardNav()\">\r\n                          <div class=\"card tile mb-3 mx-2\">\r\n                            <div class=\"d-inline-block justify-content-center align-items-center m-auto\">\r\n                              <img :src=\"topicNavUrl\" onerror=\"this.src='https://rss.com/favicon.ico'\"/>\r\n                              <br/>\r\n                              <strong class=\"mb-2\">\r\n                                <span class=\"text-secondary title\">{{ topic.title }}</span>\r\n                              </strong>\r\n                            </div>\r\n                          </div>\r\n                        </a>\r\n                      </center>\r\n                    </Transition>\r\n                  </div>\r\n              </div>\r\n            </div>\r\n        </Transition>\r\n        \r\n        <!-- Error Output Section -->\r\n        <div style=\"margin-top: -30px;\" class=\"text-danger\" v-if=\"isError && tabNav === 2\">\r\n          <p v-if=\"!data.includes('403')\">{{ data }} Try Reloading...</p>\r\n          <div v-else class=\"row\">\r\n            <div class=\"col-12\">\r\n              {{ data.replace(',','') }}\r\n              <img class=\"err\" src=\"https://rubanero14.github.io/RSS-Feed-CP-Prototype-Vue3/err.png\"/>\r\n              <figcaption>Figure 1 - Click <span>Enable Access</span> button below to open this page</figcaption>\r\n              <ul class=\"text-center text-secondary\">\r\n                <li>Click <strong class=\"text-success\">Enable Access</strong> button below</li>\r\n                <li>When new window pops up, click <strong class=\"text-success\">Request temporary access to the demo server</strong> button as per figure above.</li>\r\n                <li>Close that window and back to <span class=\"text-secondary\"><strong>RSS Feed</strong></span> page and start browsing for articles</li>\r\n              </ul>\r\n            </div>\r\n            <div class=\"col-lg-4\"></div>\r\n            <div class=\"col-lg-4\">\r\n              <a @click=\"backwardNav()\" href=\"https://cors-anywhere.herokuapp.com/corsdemo?accessRequest=01a082fe9409ff8c6c2e76a853281642569c12198c0358fadbbe4a03321d2fd7\" class=\"btn btn-outline-success w-100\" target=\"_blank\">\r\n                <i class=\"bi bi-hdd-rack\"></i> \r\n                Enable Access\r\n              </a>\r\n            </div>\r\n            <div class=\"col-lg-4\"></div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div :class=\"{'col-3':screenWidth >= 1200, '':screenWidth < 1200}\"></div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\nimport sources from '@/assets/sources.js';\r\n\r\nconst xml2js = require('xml2js');\r\n\r\nexport default {\r\n  data(){\r\n    return {\r\n      data: '',\r\n      isloading: false,\r\n      isError: false,\r\n      rssSource: '',\r\n      screenWidth: '',\r\n      tabNav: 0,\r\n      topicData:'',\r\n      topicNavUrl: '',\r\n      topicTitle: '',\r\n      topicTitle2:'',\r\n      sources: sources,\r\n    };\r\n  },\r\n  watch: {\r\n    url(){\r\n      this.getRssFeeds();\r\n      this.setScreenWidth();\r\n    },\r\n  },\r\n  mounted() {\r\n    window.addEventListener('resize', this.setScreenWidth);\r\n    this.setScreenWidth();\r\n  },\r\n  unmounted() {\r\n    window.removeEventListener('resize', this.setScreenWidth)\r\n  },\r\n  methods: {\r\n    forwardNav(data){\r\n      if(this.tabNav === 0 && data.topics.length > 1){\r\n        this.topicData = data.topics;\r\n        this.topicNavUrl = data.url;\r\n        this.topicTitle = data.name;\r\n        return this.tabNav < 3 ? this.tabNav++ : this.tabNav;\r\n      }\r\n\r\n      if(this.tabNav === 0 && data.topics.length === 1){\r\n        this.pic = data.url;\r\n        this.topicData = data.topics;\r\n        this.topicNavUrl = data.topics[0].url;\r\n        this.topicTitle2 = data.topics[0].title;\r\n        this.getRssFeeds(this.pic,this.topicNavUrl,this.topicTitle2);\r\n        return this.tabNav = 2;\r\n      }\r\n    },\r\n    backwardNav(err){\r\n      if(this.tabNav === 2 && this.topicData.length === 1){\r\n        return this.tabNav = 0;\r\n      }\r\n\r\n      if(err){\r\n        this.isError = false;\r\n        console.log('Iserror ',this.isError)\r\n        return this.tabNav = 0;\r\n      }\r\n\r\n      return this.tabNav > -1 ? this.tabNav-- : this.tabNav;\r\n    },\r\n    setScreenWidth(){\r\n      console.log(window.innerWidth);\r\n      return this.screenWidth = window.innerWidth;\r\n    },\r\n    async getRssFeeds(picUrl, payloadUrl, title){\r\n      this.tabNav = 2;\r\n      this.isError = false;\r\n      this.isloading = true;\r\n      this.pic = picUrl;\r\n      this.topicTitle2 = title;\r\n\r\n      // Payload for Fetch API setting\r\n      let payload = `https://cors-anywhere.herokuapp.com/${payloadUrl}`;\r\n      \r\n      // Fetch API as XML and convert into JSON format\r\n      this.data = await axios\r\n      .get(payload)\r\n      .then((response) => {\r\n        return xml2js.parseStringPromise(response.data);\r\n      })\r\n      .catch(err => {\r\n        this.isloading = false;\r\n        this.isError = true;\r\n        console.log(err);\r\n        return err.message + ',';\r\n      });\r\n\r\n      this.feeds = this.data.rss.channel[0].item;\r\n      \r\n      // Custom show elapsed time algorithm\r\n      this.date = () => {\r\n        // get UNIX timestamp of pubDate value\r\n        let Elapsed = new Date(this.feeds[0].pubDate).getTime();\r\n\r\n        // get UNIX timestamp of Present value\r\n        let Now = Date.now();\r\n\r\n        // Convert UNIX to seconds\r\n        let seconds = ((Now - Elapsed) / 1000);\r\n\r\n        // Convert UNIX to minutes\r\n        let minutes = (seconds / 60);\r\n\r\n        // Convert UNIX to hours\r\n        let hours = (minutes / 60);\r\n\r\n        // Convert UNIX to days\r\n        let days = (hours / 24);\r\n\r\n        // Convert UNIX to weeks\r\n        let weeks = (days / 7);\r\n\r\n        // Convert UNIX to weeks\r\n        let months = (weeks / 4);\r\n\r\n        // Convert UNIX to weeks\r\n        let years = (months / 12);\r\n\r\n        // Elapsed Time output logic\r\n        if(seconds >= 1 && seconds < 60){\r\n          return seconds.toFixed(0) + (seconds.toFixed(0) < 2 ? ' second':' seconds');\r\n        }\r\n\r\n        if(minutes >= 1 && minutes < 60){\r\n          return minutes.toFixed(0) + (minutes.toFixed(0) < 2 ? ' minute':' minutes');\r\n        }\r\n\r\n        if(hours >= 1 && hours < 24){\r\n          return hours.toFixed(0) + (hours.toFixed(0) < 2 ? ' hour':' hours');\r\n        }\r\n\r\n        if(days >= 1 && days < 7){\r\n          return days.toFixed(0) + (days.toFixed(0) < 2 ? ' day':' days');\r\n        }\r\n\r\n        if(weeks >= 1 && weeks < 4){\r\n          return weeks.toFixed(0) + (weeks.toFixed(0) < 2 ? ' week':' weeks');\r\n        }\r\n\r\n        if(months >= 1 && months < 12){\r\n          return months.toFixed(0) + (months.toFixed(0) < 2 ? ' month':' months');\r\n        }\r\n\r\n        if(years >= 1){\r\n          return years.toFixed(0) + (years.toFixed(0) < 2 ? ' year':' years');\r\n        }\r\n      };\r\n      this.isloading = false;\r\n    },\r\n  },\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n  h1 {\r\n    font-size: 50px;\r\n  }\r\n\r\n  h2 {\r\n    font-size: 25px;\r\n    font-weight: bold;\r\n  }\r\n\r\n  .tile {\r\n    min-height: 115px;\r\n    min-width: 220px;\r\n  }\r\n\r\n  .right {\r\n    border-radius: 0px 4px 4px 0px;\r\n  }\r\n\r\n  ul {\r\n    list-style: none;\r\n  }\r\n\r\n  img {\r\n    margin: 5px auto;\r\n    width: 35px;\r\n    border-radius: 4px;\r\n  }\r\n\r\n  img.logo {\r\n    height: 100%;\r\n    width: 100%;\r\n    margin: 0;\r\n    border-radius: 4px;\r\n  }\r\n\r\n  h3.title{\r\n    font-size: 18px;\r\n    font-weight:bold;\r\n    margin-bottom: 15px;\r\n  }\r\n\r\n  a.title {\r\n    text-decoration: none;\r\n    color: inherit;\r\n  }\r\n\r\n  span.time {\r\n    position: absolute;\r\n    bottom: 15px;\r\n    left: 15px;\r\n  }\r\n\r\n  .card {\r\n    cursor: pointer;\r\n    box-shadow: 0px 1px 4px 0px rgb(0 0 0 / 20%);\r\n    border-radius: 5px;\r\n    overflow: hidden;\r\n  }\r\n\r\n  .card.logo {\r\n    height: 120px;\r\n    width: 120px;\r\n    border-radius: 8px;\r\n  }\r\n\r\n  .card:hover {\r\n    background: rgba(0,0,0,.05);\r\n  }\r\n\r\n  .card.logo:hover {\r\n    box-shadow: 0px 2px 8px 8px rgb(0 0 0 / 20%)\r\n  }\r\n\r\n  .tile {\r\n    min-height: 108px;\r\n  }\r\n\r\n  .err {\r\n    width: 100%;\r\n    border: 1px solid #dc3545;\r\n  }\r\n\r\n  @media only screen and (max-width: 600px) {\r\n    span.title {\r\n      font-size: 12px;\r\n    }\r\n    .card.logo {\r\n      height: 80px;\r\n      width: 80px;\r\n      border-radius: 4px;\r\n    }\r\n  }\r\n\r\n  @media only screen and (max-width: 400px) {\r\n    .card.logo {\r\n      height: 70px;\r\n      width: 70px;\r\n    }\r\n  }\r\n\r\n  @media only screen and (max-width: 300px) {\r\n    .card.logo {\r\n      height: 60px;\r\n      width: 60px;\r\n    }\r\n  }\r\n  \r\n  .card-fade-enter-active {\r\n    transition: all 0.02s ease-in;\r\n  }\r\n\r\n  .card-fade-leave-active {\r\n    transition: all 0.02s ease-out;\r\n  }\r\n\r\n  .card-fade-enter-from,\r\n  .card-fade-leave-to {\r\n    opacity: 0;\r\n  }\r\n\r\n  .card-fade-enter-to, .card-fade-leave-from {\r\n    opacity: 0.5;\r\n  }\r\n  \r\n  /* Declared variable --i is used to dynamically calculate transition time */\r\n  .fade-enter-active {\r\n    transition: all 0.2s ease-in;\r\n    transition-delay: calc(var(--i) * 0.05s);\r\n  }\r\n\r\n  .fade-leave-active {\r\n    transition: all 0.2s ease-out;\r\n    transition-delay: calc(var(--i) * 0.02s);\r\n  }\r\n\r\n  .fade-enter-from {\r\n    opacity: 0;\r\n    transform: translateX(15px);\r\n  }\r\n\r\n  .fade-leave-to {\r\n    opacity: 0;\r\n    transform: translateX(-15px);\r\n  }\r\n\r\n  .fade-enter-to,\r\n  .fade-leave-from {\r\n    opacity: 0.5;\r\n    transform: translateX(10px);\r\n  }\r\n\r\n  /* Declared variable --j is used to dynamically calculate transition time */\r\n  .fade-articles-enter-active {\r\n    transition: all 0.2s ease-in-out;\r\n    transition-delay: calc(var(--j) * 0.02s);\r\n  }\r\n\r\n  .fade-articles-leave-active {\r\n    transition: all 0.2s ease-in-out;\r\n    transition-delay: calc(var(--j) * 0.01s);\r\n  }\r\n\r\n  .fade-articles-enter-from {\r\n    opacity: 0;\r\n    transform: translateX(-15px);\r\n  }\r\n\r\n  .fade-articles-leave-to {\r\n    opacity: 0;\r\n    transform: translateX(15px);\r\n  }\r\n\r\n  .fade-articles-enter-to,\r\n  .fade-articles-leave-from {\r\n    opacity: 1;\r\n    transform: translateX(10px);\r\n  }\r\n</style>"]},"metadata":{},"sourceType":"module"}