{"ast":null,"code":"import axios from 'axios';\n\nconst xml2js = require('xml2js');\n\nexport default {\n  data() {\n    return {\n      data: '',\n      url: '',\n      pic: '',\n      feeds: '',\n      isloading: false,\n      isError: false\n    };\n  },\n\n  computed: {\n    loadRSS() {\n      return this.getRssFeeds();\n    }\n\n  },\n  methods: {\n    async getRssFeeds() {\n      this.isError = false;\n      this.isloading = true;\n\n      let payload = () => {\n        if (this.url === 'https://tradingeconomics.com/rss/') {\n          return this.url;\n        }\n\n        return `http://preprod-secureplatformtech.com:9090/${this.url}`;\n      };\n\n      this.data = await axios.get(payload()).then(response => {\n        console.log(payload());\n        return xml2js.parseStringPromise(response.data);\n      }).catch(err => {\n        this.isloading = false;\n        this.isError = true;\n        return err.message;\n      });\n      this.feeds = this.data.rss.channel[0].item;\n      console.log(this.data);\n      this.isloading = false;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAkDA,OAAOA,KAAP,MAAkB,OAAlB;;AACA,MAAMC,MAAK,GAAIC,OAAO,CAAC,QAAD,CAAtB;;AAEA,eAAe;EACbC,IAAI,GAAE;IACJ,OAAO;MACLA,IAAI,EAAE,EADD;MAELC,GAAG,EAAE,EAFA;MAGLC,GAAG,EAAE,EAHA;MAILC,KAAK,EAAE,EAJF;MAKLC,SAAS,EAAE,KALN;MAMLC,OAAO,EAAE;IANJ,CAAP;EAQD,CAVY;;EAWbC,QAAQ,EAAE;IACRC,OAAO,GAAE;MACP,OAAO,KAAKC,WAAL,EAAP;IACF;;EAHQ,CAXG;EAgBbC,OAAO,EAAE;IACP,MAAMD,WAAN,GAAmB;MACjB,KAAKH,OAAL,GAAe,KAAf;MACA,KAAKD,SAAL,GAAiB,IAAjB;;MACA,IAAIM,OAAM,GAAI,MAAM;QAClB,IAAI,KAAKT,GAAL,KAAa,mCAAjB,EAAqD;UACnD,OAAO,KAAKA,GAAZ;QACF;;QACA,OAAQ,8CAA6C,KAAKA,GAAI,EAA9D;MACF,CALA;;MAOA,KAAKD,IAAL,GAAY,MAAMH,KAAI,CACrBc,GADiB,CACbD,OAAO,EADM,EAEjBE,IAFiB,CAEXC,QAAD,IAAc;QAClBC,OAAO,CAACC,GAAR,CAAYL,OAAO,EAAnB;QACA,OAAOZ,MAAM,CAACkB,kBAAP,CAA0BH,QAAQ,CAACb,IAAnC,CAAP;MACD,CALiB,EAMjBiB,KANiB,CAMXC,GAAE,IAAK;QACZ,KAAKd,SAAL,GAAiB,KAAjB;QACA,KAAKC,OAAL,GAAe,IAAf;QACA,OAAOa,GAAG,CAACC,OAAX;MACD,CAViB,CAAlB;MA